# Ignore artifacts:
build/
coverage/
node_modules/
dist/

# Ignore bundler output:
ios/
android/

# Ignore configuration files:
*.config.js
*.config.cjs
*.config.mjs
*.config.ts
*.config.json

# Ignore log files:
*.log

# Ignore environment variables:
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore all HTML files:
*.html

# Ignore all markdown files:
*.md

# Ignore all image files:
*.png
*.jpg
*.jpeg
*.gif
*.svg

# Ignore all binary files:
*.lock
*.zip
*.tar.gz

# Ignore build scripts:
scripts/
